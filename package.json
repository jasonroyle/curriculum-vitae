{
  "scripts": {
    "build": "npm run sass",
    "build:watch": "npm run build && npm run sass:watch",
    "sass": "node-sass src/theme.scss assets/theme.css",
    "sass:watch": "node-sass --watch --recursive src/theme.scss assets/theme.css"
  },
  "dependencies": {
    "bootstrap": "4.0.0-beta.3",
    "jquery": "3.2.1",
    "popper.js": "1.12.9"
  },
  "devDependencies": {
    "node-sass": "4.7.2"
  }
}
